generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model categorias {
  id String @id
  nombre String
  nivel Int
}

model productos{
  id String @id
  name String
  stock Int
}